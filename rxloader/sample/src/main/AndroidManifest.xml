<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="me.tatarka.rxloader.sample">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="me.tatarka.rxloader.sample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".test.RxLoaderActivity"/>
        <activity android:name=".test.RxLoaderFragmentActivity"/>
        <activity android:name=".test.RxLoaderActivityWithFragment"/>
        <activity android:name=".test.RxLoaderActivityWithSupportFragment"/>
    </application>

</manifest>
